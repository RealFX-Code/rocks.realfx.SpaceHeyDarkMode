{
    "manifest_version": 2,
    "name": "SpaceHey Dark Mode",
    "version": "9999",
    "description": "SpaceHey dark mode extension for firefox",
    "icons": {
        "48": "src/assets/icon-64.png"
    },
    "browser_specific_settings" : {
        "gecko": {
            "id": "SpaceHeyDarkMode@realfx.rocks"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.spacehey.com/*"
            ],
            "js": [
                "src/payload.js"
            ]
        }
    ]
}
